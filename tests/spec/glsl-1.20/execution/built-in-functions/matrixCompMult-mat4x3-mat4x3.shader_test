[require]
GLSL >= 1.20

[vertex shader]
varying vec4 color;
uniform mat4x3 arg0;
uniform mat4x3 arg1;
uniform int column;

void main()
{
  gl_Position = gl_Vertex;
  mat4x3 result = matrixCompMult(arg0, arg1);
  result -= -6.107;
  result *= 0.0750908599405;
  color = vec4(result[column], 0.0);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

