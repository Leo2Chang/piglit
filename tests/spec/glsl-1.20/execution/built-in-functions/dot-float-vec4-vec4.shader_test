[require]
GLSL >= 1.10

[vertex shader]
varying vec4 color;
uniform vec4 arg0;
uniform vec4 arg1;

void main()
{
  gl_Position = gl_Vertex;
  float result = dot(arg0, arg1);
  result -= -8.15265;
  result *= 0.044256795631;
  color = vec4(result, 0.0, 0.0, 0.0);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

