[require]
GLSL >= 1.10

[vertex shader]
varying vec4 color;
uniform mat2x4 arg0;
uniform mat2x4 arg1;
uniform int column;

void main()
{
  gl_Position = gl_Vertex;
  mat2x4 result = matrixCompMult(arg0, arg1);
  result -= -3.56475;
  result *= 0.113768231359;
  color = vec4(result[column]);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

