[require]
GLSL >= 1.10

[vertex shader]
varying vec4 color;
uniform vec4 arg0;

void main()
{
  gl_Position = gl_Vertex;
  vec4 result = sqrt(arg0);
  result -= -0.707106781187;
  result *= 0.353553390593;
  color = vec4(result);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

[test]
uniform vec4 arg0 0.0 0.666666666667 1.33333333333 2.0
draw rect -1 -1 2 2
probe rgba 0 0 0.25 0.538675134595 0.658248290464 0.75
