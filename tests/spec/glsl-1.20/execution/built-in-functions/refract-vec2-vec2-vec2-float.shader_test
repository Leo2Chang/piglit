[require]
GLSL >= 1.10

[vertex shader]
varying vec4 color;
uniform vec2 arg0;
uniform vec2 arg1;
uniform float arg2;

void main()
{
  gl_Position = gl_Vertex;
  vec2 result = refract(arg0, arg1, arg2);
  result -= -1.90669117207;
  result *= 0.25833234352;
  color = vec4(result, 0.0, 0.0);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

