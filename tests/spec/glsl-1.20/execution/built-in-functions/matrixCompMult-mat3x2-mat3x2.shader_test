[require]
GLSL >= 1.20

[vertex shader]
#version 120
varying vec4 color;
uniform mat3x2 arg0;
uniform mat3x2 arg1;
uniform int column;

void main()
{
  gl_Position = gl_Vertex;
  mat3x2 result = matrixCompMult(arg0, arg1);
  result -= -4.50185;
  result *= 0.0973273898741;
  color = vec4(result[column], 0.0, 0.0);
}

[fragment shader]
#version 120
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

