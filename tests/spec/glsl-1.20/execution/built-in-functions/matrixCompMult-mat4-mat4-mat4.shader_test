[require]
GLSL >= 1.10

[vertex shader]
varying vec4 color;
uniform mat4 arg0;
uniform mat4 arg1;
uniform int column;

void main()
{
  gl_Position = gl_Vertex;
  mat4 result = matrixCompMult(arg0, arg1);
  result -= -7.1834;
  result *= 0.0680272108844;
  color = vec4(result[column]);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

