[require]
GLSL >= 1.10

[vertex shader]
varying vec4 color;
uniform mat2x4 arg0;
uniform int column;

void main()
{
  gl_Position = gl_Vertex;
  mat4x2 result = transpose(arg0);
  result -= -3.175;
  result *= 0.174216027875;
  color = vec4(result[column], 0.0, 0.0);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

