[require]
GLSL >= 1.10

[vertex shader]
varying vec4 color;
uniform mat3x2 arg0;
uniform int column;

void main()
{
  gl_Position = gl_Vertex;
  mat2x3 result = transpose(arg0);
  result -= -3.335;
  result *= 0.161812297735;
  color = vec4(result[column], 0.0);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

