[require]
GLSL >= 1.10

[vertex shader]
varying vec4 color;
uniform float arg0;

void main()
{
  gl_Position = gl_Vertex;
  float result = sqrt(arg0);
  result -= -0.707106781187;
  result *= 0.353553390593;
  color = vec4(result, 0.0, 0.0, 0.0);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

[test]
uniform float arg0 0.0
draw rect -1 -1 2 2
probe rgba 0 0 0.25 0.0 0.0 0.0
uniform float arg0 0.666666666667
draw rect -1 -1 2 2
probe rgba 0 0 0.538675134595 0.0 0.0 0.0
uniform float arg0 1.33333333333
draw rect -1 -1 2 2
probe rgba 0 0 0.658248290464 0.0 0.0 0.0
uniform float arg0 2.0
draw rect -1 -1 2 2
probe rgba 0 0 0.75 0.0 0.0 0.0
