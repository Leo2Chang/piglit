[require]
GLSL >= 1.10

[vertex shader]
varying vec4 color;
uniform mat3 arg0;
uniform mat3 arg1;
uniform int column;

void main()
{
  gl_Position = gl_Vertex;
  mat3 result = matrixCompMult(arg0, arg1);
  result -= -3.2852;
  result *= 0.10294844342;
  color = vec4(result[column], 0.0);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

