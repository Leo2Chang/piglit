[require]
GLSL >= 1.10

[vertex shader]
varying vec4 color;
uniform float arg0;
uniform float arg1;

void main()
{
  gl_Position = gl_Vertex;
  float result = min(arg0, arg1);
  result -= -4.0;
  result *= 0.125;
  color = vec4(result, 0.0, 0.0, 0.0);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

