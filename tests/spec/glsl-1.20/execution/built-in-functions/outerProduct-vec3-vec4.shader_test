[require]
GLSL >= 1.20

[vertex shader]
#version 120
varying vec4 color;
uniform vec3 arg0;
uniform vec4 arg1;
uniform int column;

void main()
{
  gl_Position = gl_Vertex;
  mat4x3 result = outerProduct(arg0, arg1);
  result -= -6.4328;
  result *= 0.0746870612135;
  color = vec4(result[column], 0.0);
}

[fragment shader]
#version 120
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

