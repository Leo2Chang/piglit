[require]
GLSL >= 1.10

[vertex shader]
varying vec4 color;
uniform vec4 arg0;

void main()
{
  gl_Position = gl_Vertex;
  vec4 result = exp(arg0);
  result -= -3.49152512461;
  result *= 0.0689301411929;
  color = vec4(result);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

[test]
uniform vec4 arg0 -2.0 -0.666666666667 0.666666666667 2.0
draw rect -1 -1 2 2
probe rgba 0 0 0.25 0.276061234324 0.374928902274 0.75
