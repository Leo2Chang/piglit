[require]
GLSL >= 1.10

[vertex shader]
varying vec4 color;
uniform vec2 arg0;
uniform vec2 arg1;

void main()
{
  gl_Position = gl_Vertex;
  vec2 result = pow(arg0, arg1);
  result -= -2.0;
  result *= 0.125;
  color = vec4(result, 0.0, 0.0);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

