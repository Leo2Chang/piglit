[require]
GLSL >= 1.10

[vertex shader]
varying vec4 color;
uniform vec3 arg0;
uniform vec3 arg1;

void main()
{
  gl_Position = gl_Vertex;
  vec3 result = reflect(arg0, arg1);
  result -= -3.59577660236;
  result *= 0.144862318598;
  color = vec4(result, 0.0);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

