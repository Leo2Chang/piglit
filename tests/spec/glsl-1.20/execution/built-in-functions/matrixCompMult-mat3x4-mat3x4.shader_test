[require]
GLSL >= 1.20

[vertex shader]
#version 120
varying vec4 color;
uniform mat3x4 arg0;
uniform mat3x4 arg1;
uniform int column;

void main()
{
  gl_Position = gl_Vertex;
  mat3x4 result = matrixCompMult(arg0, arg1);
  result -= -5.5068;
  result *= 0.0815820388983;
  color = vec4(result[column]);
}

[fragment shader]
#version 120
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

