[require]
GLSL >= 1.10

[vertex shader]
varying vec4 color;
uniform mat2 arg0;
uniform mat2 arg1;
uniform int column;

void main()
{
  gl_Position = gl_Vertex;
  mat2 result = matrixCompMult(arg0, arg1);
  result -= -4.493;
  result *= 0.106337728626;
  color = vec4(result[column], 0.0, 0.0);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

