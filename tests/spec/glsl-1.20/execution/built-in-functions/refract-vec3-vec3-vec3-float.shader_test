[require]
GLSL >= 1.10

[vertex shader]
varying vec4 color;
uniform vec3 arg0;
uniform vec3 arg1;
uniform float arg2;

void main()
{
  gl_Position = gl_Vertex;
  vec3 result = refract(arg0, arg1, arg2);
  result -= -1.57557582059;
  result *= 0.317186288726;
  color = vec4(result, 0.0);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

