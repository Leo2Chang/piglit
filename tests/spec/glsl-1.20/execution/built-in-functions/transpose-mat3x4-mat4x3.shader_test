[require]
GLSL >= 1.10

[vertex shader]
varying vec4 color;
uniform mat4x3 arg0;
uniform int column;

void main()
{
  gl_Position = gl_Vertex;
  mat3x4 result = transpose(arg0);
  result -= -3.76;
  result *= 0.130890052356;
  color = vec4(result[column]);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

