[require]
GLSL >= 1.10

[vertex shader]
varying vec4 color;
uniform vec3 arg0;
uniform vec3 arg1;
uniform int column;

void main()
{
  gl_Position = gl_Vertex;
  mat3 result = outerProduct(arg0, arg1);
  result -= -4.38515;
  result *= 0.0951529107275;
  color = vec4(result[column], 0.0);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

