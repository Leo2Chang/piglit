[require]
GLSL >= 1.10

[vertex shader]
varying vec4 color;
uniform vec4 arg0;
uniform vec4 arg1;

void main()
{
  gl_Position = gl_Vertex;
  bvec4 result = lessThanEqual(arg0, arg1);
  color = vec4(result);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

[test]
uniform vec4 arg0 -1 -1 -1 -1
uniform vec4 arg1 -1 0 0 1
draw rect -1 -1 2 2
probe rgba 0 0 1 1 1 1
uniform vec4 arg0 0 0 0 0
uniform vec4 arg1 -1 0 0 1
draw rect -1 -1 2 2
probe rgba 0 0 0 1 1 1
uniform vec4 arg0 0 0 0 0
uniform vec4 arg1 -1 0 0 1
draw rect -1 -1 2 2
probe rgba 0 0 0 0 1 1
uniform vec4 arg0 1 1 1 1
uniform vec4 arg1 -1 0 0 1
draw rect -1 -1 2 2
probe rgba 0 0 0 0 0 1
