[require]
GLSL >= 1.10

[vertex shader]
varying vec4 color;
uniform vec4 arg0;

void main()
{
  gl_Position = gl_Vertex;
  vec4 result = atan(arg0);
  result -= -2.21429743559;
  result *= 0.225805256315;
  color = vec4(result);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

[test]
uniform vec4 arg0 -2.0 -0.666666666667 0.666666666667 2.0
draw rect -1 -1 2 2
probe rgba 0 0 0.25 0.367225921392 0.632774078608 0.75
