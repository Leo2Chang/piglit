[require]
GLSL >= 1.10

[vertex shader]
varying vec4 color;
uniform ivec3 arg0;
uniform ivec3 arg1;

void main()
{
  gl_Position = gl_Vertex;
  bvec3 result = lessThanEqual(arg0, arg1);
  color = vec4(result, 0.0);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

[test]
uniform ivec3 arg0 1 1 1
uniform ivec3 arg1 1 2 3
draw rect -1 -1 2 2
probe rgba 0 0 1 1 1 0
uniform ivec3 arg0 1 2 2
uniform ivec3 arg1 4 1 2
draw rect -1 -1 2 2
probe rgba 1 0 1 0 1 0
uniform ivec3 arg0 2 2 3
uniform ivec3 arg1 3 4 1
draw rect -1 -1 2 2
probe rgba 2 0 1 1 0 0
uniform ivec3 arg0 3 3 3
uniform ivec3 arg1 2 3 4
draw rect -1 -1 2 2
probe rgba 3 0 0 1 1 0
uniform ivec3 arg0 4 4 4
uniform ivec3 arg1 1 2 3
draw rect -1 -1 2 2
probe rgba 4 0 0 0 0 0
uniform ivec3 arg0 4 1 1
uniform ivec3 arg1 4 1 2
draw rect -1 -1 2 2
probe rgba 5 0 1 1 1 0
