[require]
GLSL >= 1.10

[vertex shader]
varying vec4 color;
uniform vec2 arg0;
uniform float arg1;

void main()
{
  gl_Position = gl_Vertex;
  vec2 result = mod(arg0, arg1);
  result -= -3.8;
  result *= 0.131578947368;
  color = vec4(result, 0.0, 0.0);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

