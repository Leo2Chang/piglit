[require]
GLSL >= 1.10

[vertex shader]
varying vec4 color;
uniform vec4 arg0;
uniform vec2 arg1;
uniform int column;

void main()
{
  gl_Position = gl_Vertex;
  mat2x4 result = outerProduct(arg0, arg1);
  result -= -4.464;
  result *= 0.116387337058;
  color = vec4(result[column]);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

