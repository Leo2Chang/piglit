[require]
GLSL >= 1.10

[vertex shader]
varying vec4 color;
uniform vec4 arg0;
uniform vec4 arg1;
uniform float arg2;

void main()
{
  gl_Position = gl_Vertex;
  vec4 result = refract(arg0, arg1, arg2);
  result -= -1.67691614478;
  result *= 0.296196403557;
  color = vec4(result);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

