[require]
GLSL >= 1.10

[vertex shader]
varying vec4 color;
uniform float arg0;
uniform float arg1;
uniform float arg2;

void main()
{
  gl_Position = gl_Vertex;
  float result = smoothstep(arg0, arg1, arg2);
  result -= -0.5;
  result *= 0.5;
  color = vec4(result, 0.0, 0.0, 0.0);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

