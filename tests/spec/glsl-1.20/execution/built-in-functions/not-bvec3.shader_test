[require]
GLSL >= 1.10

[vertex shader]
varying vec4 color;
uniform bvec3 arg0;

void main()
{
  gl_Position = gl_Vertex;
  bvec3 result = not(arg0);
  color = vec4(result, 0.0);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

[test]
uniform ivec3 arg0 0 1 0
draw rect -1 -1 2 2
probe rgba 0 0 1.0 0.0 1.0 0.0
