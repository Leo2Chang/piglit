[require]
GLSL >= 1.20

[vertex shader]
varying vec4 color;
uniform vec4 arg0;
uniform vec3 arg1;
uniform int column;

void main()
{
  gl_Position = gl_Vertex;
  mat3x4 result = outerProduct(arg0, arg1);
  result -= -6.4328;
  result *= 0.0746870612135;
  color = vec4(result[column]);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

