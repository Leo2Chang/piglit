[require]
GLSL >= 1.10

[vertex shader]
varying vec4 color;
uniform vec2 arg0;
uniform vec2 arg1;
uniform vec2 arg2;

void main()
{
  gl_Position = gl_Vertex;
  vec2 result = faceforward(arg0, arg1, arg2);
  result -= -2.4;
  result *= 0.208333333333;
  color = vec4(result, 0.0, 0.0);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

