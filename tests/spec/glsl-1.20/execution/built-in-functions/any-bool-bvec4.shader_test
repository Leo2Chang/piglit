[require]
GLSL >= 1.10

[vertex shader]
varying vec4 color;
uniform bvec4 arg0;

void main()
{
  gl_Position = gl_Vertex;
  bool result = any(arg0);
  color = vec4(result, 0.0, 0.0, 0.0);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

