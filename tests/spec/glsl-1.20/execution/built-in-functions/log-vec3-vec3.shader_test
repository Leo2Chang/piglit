[require]
GLSL >= 1.10

[vertex shader]
varying vec4 color;
uniform vec3 arg0;

void main()
{
  gl_Position = gl_Vertex;
  vec3 result = log(arg0);
  result -= -7.25432886926;
  result *= 0.0943695829089;
  color = vec4(result, 0.0);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

[test]
uniform vec3 arg0 0.01 0.673333333333 1.33666666667
draw rect -1 -1 2 2
probe rgba 0 0 0.25 0.647263425112 0.711972056407 0.0
uniform vec3 arg0 2.0 0.01 0.673333333333
draw rect -1 -1 2 2
probe rgba 0 0 0.75 0.25 0.647263425112 0.0
