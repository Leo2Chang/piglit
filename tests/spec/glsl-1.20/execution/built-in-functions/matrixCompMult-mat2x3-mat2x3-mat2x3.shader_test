[require]
GLSL >= 1.10

[vertex shader]
varying vec4 color;
uniform mat2x3 arg0;
uniform mat2x3 arg1;
uniform int column;

void main()
{
  gl_Position = gl_Vertex;
  mat2x3 result = matrixCompMult(arg0, arg1);
  result -= -5.0246;
  result *= 0.0853417081996;
  color = vec4(result[column], 0.0);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

