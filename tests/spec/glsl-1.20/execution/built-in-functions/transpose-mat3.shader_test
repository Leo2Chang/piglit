[require]
GLSL >= 1.20

[vertex shader]
#version 120
varying vec4 color;
uniform mat3 arg0;
uniform int column;

void main()
{
  gl_Position = gl_Vertex;
  mat3 result = transpose(arg0);
  result -= -3.535;
  result *= 0.135501355014;
  color = vec4(result[column], 0.0);
}

[fragment shader]
#version 120
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

