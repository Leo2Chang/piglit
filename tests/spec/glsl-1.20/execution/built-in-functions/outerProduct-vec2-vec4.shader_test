[require]
GLSL >= 1.10

[vertex shader]
varying vec4 color;
uniform vec2 arg0;
uniform vec4 arg1;
uniform int column;

void main()
{
  gl_Position = gl_Vertex;
  mat4x2 result = outerProduct(arg0, arg1);
  result -= -4.464;
  result *= 0.116387337058;
  color = vec4(result[column], 0.0, 0.0);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

