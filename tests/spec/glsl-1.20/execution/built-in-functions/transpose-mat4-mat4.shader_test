[require]
GLSL >= 1.10

[vertex shader]
varying vec4 color;
uniform mat4 arg0;
uniform int column;

void main()
{
  gl_Position = gl_Vertex;
  mat4 result = transpose(arg0);
  result -= -3.89;
  result *= 0.129533678756;
  color = vec4(result[column]);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

