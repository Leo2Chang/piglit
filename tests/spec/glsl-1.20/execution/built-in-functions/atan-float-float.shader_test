[require]
GLSL >= 1.10

[vertex shader]
varying vec4 color;
uniform float arg0;
uniform float arg1;

void main()
{
  gl_Position = gl_Vertex;
  float result = atan(arg0, arg1);
  result -= -5.10508806208;
  result *= 0.0909456817668;
  color = vec4(result, 0.0, 0.0, 0.0);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

