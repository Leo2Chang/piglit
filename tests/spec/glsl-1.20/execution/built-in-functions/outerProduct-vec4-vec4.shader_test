[require]
GLSL >= 1.20

[vertex shader]
varying vec4 color;
uniform vec4 arg0;
uniform vec4 arg1;
uniform int column;

void main()
{
  gl_Position = gl_Vertex;
  mat4 result = outerProduct(arg0, arg1);
  result -= -6.6392;
  result *= 0.0723651836628;
  color = vec4(result[column]);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

