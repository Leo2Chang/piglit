[require]
GLSL >= 1.20

[vertex shader]
varying vec4 color;
uniform mat3x4 arg0;
uniform int column;

void main()
{
  gl_Position = gl_Vertex;
  mat4x3 result = transpose(arg0);
  result -= -3.78;
  result *= 0.131578947368;
  color = vec4(result[column], 0.0);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

