[require]
GLSL >= 1.10

[vertex shader]
varying vec4 color;
uniform vec4 arg0;
uniform vec4 arg1;

void main()
{
  gl_Position = gl_Vertex;
  vec4 result = reflect(arg0, arg1);
  result -= -4.24108172556;
  result *= 0.108174452351;
  color = vec4(result);
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

