[require]
GLSL >= 1.20

[vertex shader]
#version 120
void f(out int x)
{
  x = 4;
}

void main()
{
  gl_Position = gl_Vertex;
  float value;
  f(value);
  gl_FrontColor = vec4(1.0/value);
}

[fragment shader]
void main()
{
  gl_FragColor = gl_Color;
}

[test]
draw rect -1 -1 2 2
probe all rgba 0.25 0.25 0.25 0.25
